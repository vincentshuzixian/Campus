.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.btn-primary {
  background: #07c160;
  color: #fff;
  border: none;
  border-radius: 4px;
}

.btn-primary:active {
  background: #06ad56;
}

.page {
  min-height: 100vh;
  background: #f8f8f8;
} 